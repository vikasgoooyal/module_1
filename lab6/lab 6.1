import java.util.Scanner;

public class BlaDe {
	
	String first;
	String second;
	
	public void details() throws NameException{
		
		System.out.println("Enter first name");
		Scanner sc = new Scanner (System.in);
		first = sc.nextLine();
		System.out.println("Enter second name");
		second = sc.nextLine();
		
		if(first.equals("") || second.equals(""))
		{
			throw new NameException("vikas");
		}
		else
		
			System.out.println("name in correct form");
	}
}

